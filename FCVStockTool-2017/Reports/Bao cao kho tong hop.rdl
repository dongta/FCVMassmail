    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@cateid">
            <Value>=Parameters!cateid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@productid">
            <Value>=Parameters!productid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@stockstatusid">
            <Value>=Parameters!stockstatusid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        IT.Id, pr.Code, pr.Name,it.Name as PCName,it.SerialNo, it.PODate, cate.Name as Category, emp.Name as Employee,de.Name as Department,bu.Name as Building,case when it.EmployeeId is null then 1 else 0 end as 'NotOwn',
case when it.EmployeeId is not null then 1 else 0 end as 'Onwned', stockSt.Text as StockStatus,proSt.Text as ProductStatus
FROM          Products as pr  inner join  ProductItems as it  on it.ProductId = pr.Id
left outer join Categories as cate on pr.CategoryId = cate.Id
left outer join StockStatuses as stockSt on it.StockStatusId = stockSt.Id
left outer join ProductStatuses as proSt on it.ProductStatusId = proSt.Id
left outer join Employees as emp on it.EmployeeId = emp.Id
left outer join Buildings as bu on it.BuildingId = bu.Id
left outer join Departments as de on emp.DepartmentId = de.Id
where cate.Id in(@cateid) and pr.Id  in (@productid) and ((@stockstatusid= 0) or (it.StockStatusId = @stockstatusid))</CommandText>
      </Query>
