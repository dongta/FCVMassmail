  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@cateid">
            <Value>=Parameters!cateid.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@id">
            <Value>=Parameters!id.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT acc.Id, acc.Name, cate.Name as CategoryName, acc.Amount,
ex.id as ImportId,ex.Amount as ImportAmount, ex.ImportOn, us.DisplayName as ExportUser
  FROM [dbo].[Accessories] as acc
  INNER JOIN dbo.Categories as cate on acc.CategoryId = cate.Id
  LEFT OUTER JOIN dbo.AccessoryImports as ex on acc.Id = ex.AccessoryId and ex.Cancel = 0
  LEFT OUTER JOIN dbo.Users as us on ex.ImportByUserId = us.Id
  WHERE acc.CategoryId in (@cateid) and acc.Id in (@id)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
