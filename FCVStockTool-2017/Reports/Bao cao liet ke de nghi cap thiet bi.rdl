      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@from">
            <Value>=Parameters!from.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@to">
            <Value>=Parameters!to.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        pt.Code, cate.Name as Category, Reason, reU.DisplayName, RequestOn, emp.Name as Employee, reA.DisplayName AS ApproveBy, ApproveOn, reE.DisplayName as ExportBy, ExportOn, reC.DisplayName as CancelBy, CancelOn, SerialNo, pt.Description, 
                         TicketStatusId, pt.Id
FROM            ProductTickets as pt inner join Categories as cate
on pt.CategoryId = cate.Id
left outer join Users  as reU on pt.RequestById = reU.Id
left outer join Employees as emp on pt.EmployeeId = emp.Id
left outer join Users  as reA on pt.ApproveById = reA.Id
left outer join Users  as reE on pt.ExportById = reE.Id
left outer join Users  as reC on pt.CancelById = reC.Id
WHERE DATEADD(DAY,DATEDIFF(day,0,RequestOn),0) between  DATEADD(DAY,DATEDIFF(day,0,@from),0)   and DATEADD(DAY,DATEDIFF(day,0,@to),0)</CommandText>
      </Query>
