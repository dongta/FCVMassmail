  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT acc.Id, acc.Name, cate.Name as CategoryName, acc.Amount,
ex.Amount as ExportAmount, ex.ExportOn,
imp.Amount as ImportAmount, imp.ImportOn
  FROM [dbo].[Accessories] as acc
  INNER JOIN dbo.Categories as cate on acc.CategoryId = cate.Id
  LEFT OUTER JOIN dbo.AccessoryExports as ex on acc.Id = ex.AccessoryId and ex.Cancel = 0
  LEFT OUTER JOIN dbo.AccessoryImports as imp on acc.Id = imp.AccessoryId and imp.Cancel = 0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
