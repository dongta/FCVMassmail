  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        pr.Id, it.ShortCode, pr.Name, cate.Name as Category, emp.Name as EmpName, emp.Code as EmpCode, dep.Name as DeptName
FROM          Products as pr  inner join  ProductItems as it  on it.ProductId = pr.Id
left outer join Categories as cate on pr.CategoryId = cate.Id
left outer join Employees as emp on it.EmployeeId = emp.Id
left outer join Departments as dep on emp.DepartmentId = dep.Id</CommandText>
      </Query>
